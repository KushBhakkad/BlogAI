<body>
    <div class="content">      
      <div class="container">
        <%- include('header') %>
      
        <main class="container">
            <div class="row g-5">
                <div class="col-md-12">
                    <h3 class="pb-4 my-4 fst-italic border-bottom">
                        Archive for "<%= monthYear %>"
                    </h3>
                    <% if (posts.length > 0) { %>
                        <% posts.forEach(post => { %>
                            <article class="blog-post">
                                <a class="text-decoration-none" href="/posts/<%= post.id %>"><h2 class="display-5 link-body-emphasis mb-1"><%= post.title %></h2></a>
                                <p class="blog-post-meta"><%= post.date %> by <%= post.author %></p>
                                <p class="truncate"><%= post.content %></p>
                                <hr>
                            </article> 
                        <% }); %>   
                    </div>
                    <% } else { %>
                        <p>No posts found for this month and year.</p>
                    <% } %>
            </div>
        </main>        
    </div>    
    
    <footer class="py-5 text-center text-body-secondary bg-body-tertiary">
      <p>A Generative AI Blog website built by <a href="https://www.linkedin.com/in/kushbhakkad/">@KushBhakkad</a>.</p>
      <p class="mb-0">
        <a href="/">Back to top</a>
      </p>
    </footer>


<script>
  document.addEventListener('DOMContentLoaded', function() {
      const navLinks = document.querySelectorAll('.nav-item');

      navLinks.forEach(link => {
          link.addEventListener('click', function() {
              // Remove 'active' class from all links
              navLinks.forEach(nav => nav.classList.remove('active'));

              // Add 'active' class to the clicked link
              this.classList.add('active');
          });
      });
  });
</script>

